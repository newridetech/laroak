{
    "name": "newridetech/laroak",
    "description": "The Laroak Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "alsofronie/eloquent-uuid": "^1.0",
        "doctrine/dbal": "^2.7",
        "felixkiss/uniquewith-validator": "^3.1",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "leafo/scssphp": "^0.7.6",
        "newridetech/laravel-scss": "^1.0",
        "newridetech/php-classnames": "^1.2",
        "watson/validating": "^3.1",
        "webmozart/assert": "^1.3",
        "wikimedia/composer-merge-plugin": "^1.4"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Newride\\Laroak\\app\\": "app/",
            "Newride\\Laroak\\bundles\\": "bundles/",
            "Newride\\Laroak\\extensions\\": "extensions/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Newride\\Laroak\\tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "merge-plugin": {
            "include": [
                "bundles/*/composer.json",
                "extensions/*/composer.json"
            ]
        }
    }
}
